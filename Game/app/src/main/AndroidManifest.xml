<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhf.game">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/game"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--注册-->
        <activity android:name=".ui.RegistActivity"/>
        <!--开始-->
        <activity android:name=".ui.GoOnActivity"/>
        <!--选择头像-->
        <activity android:name=".ui.SelectIconAcitivty"/>
        <!--关卡-->
        <activity
            android:name=".ui.LevelActivity"
            android:theme="@style/myStyle" />

        <activity android:name=".MainActivity"/>
        <!--登录-->
        <activity android:name=".ui.LoginAcivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>